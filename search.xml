            <pre class="aplayer-lrc-content"></pre>
        </div>
        <script>
          var ap = new APlayer({
            element: document.getElementById("aplayer-sjKvtRXu"),
            narrow: false,
            autoplay: true,
            showlrc: false,
            music: {
              title: "猎户星座",
              author: "陈二",
              url: "./028.aac",
              pic: "",
              lrc: ""
            }
          });
          window.aplayers || (window.aplayers = []);
          window.aplayers.push(ap);
